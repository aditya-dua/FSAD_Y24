package spring_intro;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class SpringMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		ApplicationContext context=
				new ClassPathXmlApplicationContext("spring-config.xml");
		Greeting greet = (Greeting) context.getBean("greeting");
		
		greet.display();
		
		Address add= (Address) context.getBean("address");
		add.display();
		System.out.println(add);
	
		
		Address add1= (Address) context.getBean("address");
		add1.display();
		System.out.println(add1);

		
		Address addC= (Address) context.getBean("addressCons");
		addC.display();
		System.out.println(addC);

		Address addC1= (Address) context.getBean("addressCons");
		addC1.display();
		System.out.println(addC1);
		
		Address addC2= (Address) context.getBean("addressCons2");
		addC2.display();
		System.out.println(addC2);
		
		Address addC21= (Address) context.getBean("addressCons2");
		addC2.display();
		System.out.println(addC2);

	}
	/*
	 * Num - 123 City: nullstate : Delhi
spring_intro.Address@2aa5fe93
Num - 123 City: nullstate : Delhi
spring_intro.Address@2aa5fe93
Num - 1234 City: nullstate : null
spring_intro.Address@71c8becc
Num - 1234 City: nullstate : null
spring_intro.Address@19d37183
	 */

}
